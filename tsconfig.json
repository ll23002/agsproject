{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "types": [
      "@girs/glib-2.0",
      "@girs/gio-2.0",
      "@girs/cairo-1.0",
      "@girs/pango-1.0",
      "@girs/gtop-2.0",
      "ags"
    ],
    "strict": true,
    "module": "ES2022",
    "target": "ES2020",
    "lib": ["ES2023", "DOM"],
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "ags/gtk4"
  }
}